execute as @e[type=area_effect_cloud,tag=spawner,sort=random,limit=1] at @s run function items:avalanche/spread_snow

# killing snow above avalanche safe points
execute as @e[type=marker,tag=avalanche_point] at @s positioned ~-1 ~ ~-1 run kill @e[type=item,nbt={Item:{id:"minecraft:snow_block"}},dx=2,dy=50,dz=2]

execute as @a[tag=playing] at @s positioned ~ 66 ~ unless entity @e[type=marker,tag=avalanche_point,distance=..3] run effect give @s instant_damage 1 1 true

